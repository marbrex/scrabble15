                    <Cursor fx:constant="DEFAULT"/>
                  </cursor>
                </ImageView>
              </graphic>
            </JFXButton>
          </children>
        </HBox>
        <HBox id="letters-block" fx:id="lettersBlock" alignment="CENTER" fillHeight="false"
          prefHeight="80.0" prefWidth="589.0" spacing="10.0"/>
